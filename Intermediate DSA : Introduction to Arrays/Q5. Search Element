def main():
    # YOUR CODE GOES HERE
    # Please take input and print output to standard input/output (stdin/stdout)
    # E.g. 'input()/raw_input()' for input & 'print' for output

    T = int(input())
    output=[0]*10

    for i in range(T):
        output[i]=0
        N=int(input())
        A=list(map(int, input().split()))
        B=int(input())
        for j in range(N):
            if B==A[j]:
                output[i]=1
                break

    for i in range(T):
        print(output[i])

    return 0

if __name__ == '__main__':
    main()
