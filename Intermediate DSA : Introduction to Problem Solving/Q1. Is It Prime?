def main():
    # YOUR CODE GOES HERE
    # Please take input and print output to standard input/output (stdin/stdout)
    # E.g. 'input()/raw_input()' for input & 'print' for output

    isPrime="YES"
    inputNumber = int(input())

    for i in range(2,inputNumber):
        if(inputNumber%i==0):
            isPrime="NO"
            break

    print(isPrime)
    return 0

if __name__ == '__main__':
    main()
