def main():
    # YOUR CODE GOES HERE
    # Please take input and print output to standard input/output (stdin/stdout)
    # E.g. 'input()/raw_input()' for input & 'print' for output

    ###
    T=int(input())
    N=[]
    for i in range(T):
     N.append(int(input()))

    for i in N:
     sum=0
     for j in range(1, i):
      if i%j==0:
       sum+=j
     if sum==i:
      print("YES")
     else:
      print("NO")

    ###   
    
    return 0

if __name__ == '__main__':
    main()
